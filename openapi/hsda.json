---
layout: none
---
{% assign openapi = site.data.api-commons.hsda %}swagger: {{ openapi.swagger }}
info:
  title: {{ openapi.info.title }}
  description: {{ openapi.info.description }}
  termsOfService: {{ openapi.info.termsOfService }}
  contact:
    name: {{ openapi.contact.name }}
    url: {{ openapi.contact.url }}
    email: {{ openapi.contact.email }}
  license:
    name: {{ openapi.license.name }}
    url: {{ openapi.license.url }}
  version: {{ openapi.info.version }}
host: {{ openapi.host }}
basePath: {{ openapi.basePath }}
schemes:
{% for scheme in openapi.schemes %}- {{ scheme }} {% endfor %}
produces:
{% for produce in openapi.produces %}- {{ produce }} {% endfor %}
{% for path in openapi.paths %}
  {{ path }}<br />
{% endfor %}
